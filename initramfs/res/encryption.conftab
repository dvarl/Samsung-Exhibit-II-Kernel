# mount point   device name   device mapper path   device mapper name   file sysystem   protect type
/cache /dev/block/mmcblk0p16 /dev/mapper/cache cache ext4 NOT_PROTECTED 
/efs /dev/block/mmcblk0p27 /dev/mapper/efs efs ext4 NOT_PROTECTED 	
/data /dev/block/mmcblk0p17 /dev/mapper/data data ext4 PROTECTED_WITH_PASSWORD 
/sdcard /dev/block/mmcblk0p28 /dev/mapper/sdcard sdcard vfat PROTECTED_WITH_PASSWORD
/ext_sdcard /dev/block/mmcblk1 /dev/mapper/extsdcard sdcard1 vfat PROTECTED_WITH_PASSWORD 
